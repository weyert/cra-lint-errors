{
  "name": "cra-lint-errors",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint .",
    "lint:teamcity": "ESLINT_TEAMCITY_DISPLAY_CONFIG=true ESLINT_TEAMCITY_REPORTER=inspections ./node_modules/.bin/eslint . --format './node_modules/eslint-teamcity/index.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-teamcity": "^2.2.0"
  }
}
